   vector<int> subsetSums(vector<int> arr, int N){
        vector<int>ans;
        int k=pow(2,N)-1;
        while(k>=0){
            int sum=0;
            for(int i=0;i<N;i++){
                if(k>>i&1)  sum+=arr[i];
            }
            k--;
            ans.push_back(sum);
        }
        sort(ans.begin(),ans.end());
        return ans;
    }
